{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "connection",
          "style": "powerline",
          "foreground": "#fff",
          "template": "{{ if eq .Type \"wifi\"}} \uf1eb  {{ else if eq .Type \"ethernet\"}}\ueba9{{ end }}"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 3
}
